{
  "name": "Desafio-entregable",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.js",
  "scripts": {
    "start": "node -r dotenv/config . --port 8080",
    "start:cluster": "node -r dotenv/config . --port 8080 --mode cluster",
    "dev": "nodemon --legacy-watch --ignore './public/*' -r dotenv/config . --port 8080",
    "dev:cluster": "nodemon --legacy-watch --ignore './public/*' -r dotenv/config . --port 8080 --mode cluster",
    "forever:fork": "forever start --watch . --port 8080",
    "forever:cluster": "forever start --watch . --port 8080 --mode cluster",
    "pm2:worker": "pm2 start worker.config.js",
    "pm2:cluster": "pm2 start cluster.config.js",
    "test:artillery": "artillery quick --count=50 -n 20 http://localhost:8080/info > result_artillery.txt"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "artillery": "^2.0.0-15",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie": "^0.4.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.3",
    "express-flash": "^0.0.2",
    "express-handlebars": "^6.0.3",
    "express-session": "^1.17.2",
    "faker": "^5.5.3",
    "forever": "^4.0.3",
    "handlebars": "^4.7.7",
    "log4js": "^6.4.6",
    "moment": "^2.29.2",
    "mongoose": "^6.2.9",
    "nodemon": "^2.0.15",
    "normalizr": "^3.6.2",
    "parser": "^0.1.4",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "pm2": "^5.2.0",
    "socket.io": "^4.4.1",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "dotenv": "^16.0.0"
  }
}
