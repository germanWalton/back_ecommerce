    {{#if data}}
    {{#each data}}
<div class="card mx-auto text-center" style="width: 18rem;">
  <img src="{{this.thumbnail}}" class="w-25 mx-auto card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <p class="card-text">${{this.price}}</p>
  </div>
</div>
  {{/each}}
   {{else}}
   <h5 class="text-center mt-5">Upss.... no se encontraron productos</h5>
   {{/if}}